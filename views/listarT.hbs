                    </tr>
                </thead>
                <tbody>


                    {{#each tematicas}}

                    <tr>
                        <td>{{nombre}}</td>
                        <td>{{tipo}}</td>
                        <td>
                            <div id="actionsContainer">
                                <div id="actiosItems">
                                   
                                    <span id="span2"><a class="nav-link" href="#" onclick="">
                                            <i class="fa fa-trash"></i>
                                        </a></span>
                                </div>
                            </div>
                        </td>


                    </tr>
                     {{/each}}
                </tbody>
            </table>

            </form>

        </div>
    </div>
</div>
